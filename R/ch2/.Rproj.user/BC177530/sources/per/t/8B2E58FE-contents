# 데이터프레임(테이블) 자료구조
a <- c(1, 2)
b <- c(3, 4)
c <- c(T, F)

df1 <- data.frame(a, b, c)
df1

df2 <- data.frame(num = c(1,2,3),
                  name = c('김춘추','김유신','장보고'),
                  age = c(21, 20, 32)
                  )
df2

# 데이터 추출
name1 <- df2$name[1]
name1

name2 <- df2$name[2]
name2

# 행렬 -> 데이터프레임 변환
M <- matrix(c(1:9),nrow=3,byrow=T)
M
df3 <- data.frame(M)
df3
